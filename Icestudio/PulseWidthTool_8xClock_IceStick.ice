            {
              "id": "2f2e567e-ed28-45ae-9fd8-f89f8a1e6d92",
              "type": "basic.info",
              "data": {
                "info": "---\n# This is an I2C Slave Block for iCEstudio\n\n### https://www.chrisbot.com/ice40-i2c/\n---",
                "readonly": true
              },
              "position": {
                "x": 544,
                "y": -296
              },
              "size": {
                "width": 608,
                "height": 144
              }
            },
            {
              "id": "f6ddf2db-026c-47dd-8dd5-79ec06223ebe",
              "type": "basic.code",
              "data": {
                "code": "// I2C Slave block. From here: https://www.chrisbot.com/ice40-i2c/\r\n\r\n// NOTE: I removed the restriction from the original which limited register\r\n// ids to less than 6 - RM\r\n\r\nlocalparam  IDLE     = 0,\r\n            START    = 1,\r\n            STOP     = 2,\r\n            DATA     = 3,\r\n            ID       = 4,\r\n            REGISTER = 5,\r\n            WRITE    = 6,\r\n            READ     = 7;\r\n\r\n//localparam TO = 1600; // timeout = 0.1ms = 10 scl\r\nlocalparam TO = 9600; // timeout = 0.1ms = 10 scl\r\nlocalparam PE = 2'b01;\r\nlocalparam NE = 2'b10;\r\n\r\nreg S, P, active, read, data_strobe, sda_out, oe;\r\nreg  [3:0] edges;\r\nreg  [7:0] sr;\r\nreg  [2:0] bctr;\r\nreg  [2:0] state;\r\nreg  [2:0] next_state;\r\nreg  [7:0] register;\r\nreg  [7:0] data_out;\r\nreg [10:0] timer;\r\n\r\nreg idle = 0;\r\n\r\nwire sda_in = edges[2];\r\n\r\n// Edge Detector\r\nalways @(posedge clk) begin\r\n    edges[3] <= edges[2];\r\n    edges[2] <= sda;\r\n    edges[1] <= edges[0];\r\n    edges[0] <= scl;\r\nend\r\n\r\n// Start-Stop Detector\r\nalways @(posedge clk) begin\r\n    // Start Detector\r\n    if (edges == 4'b1011) begin\r\n        S <= 1;\r\n        active <= 1;\r\n    end\r\n    else\r\n        S <= 0;\r\n    // Stop Detector\r\n    if (edges == 4'b0111) begin\r\n        P <= 1;\r\n        active <= 0;\r\n    end\r\n    else\r\n        P <= 0;\r\nend\r\n\r\nalways @(posedge clk) begin\r\n    // Start-Stop State Setter\r\n    if (P)\r\n        state <= STOP;\r\n    if (S)\r\n        state <= START;\r\n\r\n    // Timeout timer\r\n    if (active && (edges[1:0] == PE))\r\n        timer <= 0;\r\n    else begin\r\n        timer <= timer + 1;\r\n        if (timer > TO)\r\n            state <= STOP;\r\n    end\r\n\r\n    // State machine\r\n    case (state) // Always\r\n        START: begin\r\n            state <= DATA;\r\n            next_state <= ID;\r\n            bctr = 0;\r\n        end\r\n        STOP: begin\r\n            state <= IDLE;\r\n            oe <= 0;\r\n            read <= 0;\r\n            data_strobe <= 0;\r\n            timer <= 0;\r\n        end\r\n    endcase\r\n\r\n    if (edges[1:0] == NE) // Falling edge of scl\r\n        case (state)\r\n            DATA: begin\r\n                oe <= read;\r\n                data_strobe <= 0;\r\n                sda_out = sr[7];\r\n            end\r\n            ID:\r\n                if (sr[7:1] == id) begin\r\n                    oe <= 1;\r\n                    sda_out <= 0;\r\n                    read <= sr[0];\r\n                end\r\n                else\r\n                    state <= IDLE;\r\n            REGISTER:\r\n                if (sr < 6) begin\r\n                    oe <= 1;\r\n                    sda_out <= 0;\r\n                end\r\n                else\r\n                    state <= IDLE;\r\n            WRITE: begin\r\n                data_out <= sr;\r\n                oe <= 1;\r\n                sda_out <= 0;\r\n            end\r\n            READ:\r\n                oe <= 0;\r\n        endcase\r\n\r\n    if (edges[1:0] == PE) // Rising edge of scl\r\n        case (state)\r\n            DATA: begin\r\n                sr <= sr << 1 | sda_in;\r\n                bctr <= bctr + 1;\r\n                if (bctr == 7) begin\r\n                    bctr <= 0;\r\n                    state <= next_state;\r\n                end\r\n            end\r\n            ID:\r\n                if (read) begin\r\n                    data_strobe <= 1;\r\n                    sr <= data_in;\r\n                    state <= DATA;\r\n                    next_state <= READ;\r\n                end\r\n                else begin\r\n                    state <= DATA;\r\n                    next_state <= REGISTER;\r\n                end\r\n            REGISTER: begin\r\n                register <= sr;\r\n                state <= DATA;\r\n                next_state <= WRITE;\r\n            end\r\n            WRITE: begin\r\n                data_strobe <= 1;\r\n                state <= DATA;\r\n            end\r\n            READ:\r\n                if (sda_in == 1)\r\n                    state <= IDLE;\r\n                else begin\r\n                    data_strobe <= 1;\r\n                    sr <= data_in;\r\n                    state <= DATA;\r\n                end\r\n        endcase\r\nend\r\n",
                "params": [
