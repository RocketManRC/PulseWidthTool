            {
              "id": "0567c80f-1ae2-4735-a8bf-bdd28d5846fa",
              "type": "basic.code",
              "data": {
                "code": "// synchronous counter with async reset\n// http://fullchipdesign.com/verilog_free_counter_resetn.htm\n\nreg [2:0]c;\n\nalways @(posedge cnt or negedge rst)\nbegin\n  if( rst == 0 ) c <= 0;\n  else c <= c + 1;\nend\n\nassign count = c;",
                "params": [],
                "ports": {
                  "in": [
                    {
                      "name": "rst"
                    },
                    {
                      "name": "cnt"
                    }
                  ],
                  "out": [
                    {
                      "name": "count",
                      "range": "[2:0]",
                      "size": 3
                    }
                  ]
                }
              },
              "position": {
                "x": 288,
                "y": 248
              },
              "size": {
                "width": 360,
                "height": 176
              }
            },
            {
              "id": "45b498be-628d-4f3f-9fb3-93c2d75e856d",
              "type": "basic.code",
              "data": {
                "code": "// This outputs the byte selected\n// by the index (0 to 7)\n\nreg [7:0] out;\n\nalways @(*)\nbegin\n    case( index )\n        3'd0: out = in64[7:0];\n        3'd1: out = in64[15:8];\n        3'd2: out = in64[23:16];\n        3'd3: out = in64[31:24];\n        3'd4: out = in64[39:32];\n        3'd5: out = in64[47:40];\n        3'd6: out = in64[55:48];\n        3'd7: out = in64[63:56];\n        default: out = 0;\n    endcase\nend\n    \nassign out8 = out;\n",
                "params": [],
                "ports": {
                  "in": [
                    {
                      "name": "in64",
                      "range": "[63:0]",
                      "size": 64
                    },
                    {
                      "name": "index",
                      "range": "[2:0]",
                      "size": 3
                    }
                  ],
                  "out": [
                    {
                      "name": "out8",
                      "range": "[7:0]",
                      "size": 8
                    }
                  ]
                }
              },
              "position": {
                "x": 776,
