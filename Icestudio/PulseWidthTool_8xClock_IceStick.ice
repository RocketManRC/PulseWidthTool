            {
              "id": "fa70efeb-882e-4cd5-aa9f-99afac42f5b8",
              "type": "basic.input",
              "data": {
                "name": "length",
                "clock": false
              },
              "position": {
                "x": 72,
                "y": 368
              }
            },
            {
              "id": "0abef30b-06f3-4418-93df-9086b7864f66",
              "type": "basic.code",
              "data": {
                "code": "// This is a pulse width counter (pwc)\n// It starts counting on the pos edge of\n// the input pulse and latches the count to data\n// on the falling edge if length is 1.\n// The counter is always reset on the rising edge\n// of the input pulse so if length is 0 it measures\n// the period.\n\nreg [63:0] counter;\nreg [63:0] width;\nreg lastPulse;\n\nalways @(posedge clk)\nbegin\n    if( pulse && ~lastPulse )\n    begin\n        if( length )\n            counter <= 0;\n            \n        lastPulse <= 1;\n    end\n    else if( ~pulse && lastPulse )\n    begin\n        width <= counter;\n        counter <= 0;\n        lastPulse <= 0;\n    end\n    else\n    begin\n        counter <= counter + 1;\n        lastPulse <= pulse;\n    end\n\nend\n\nassign data = width;\n\n",
                "params": [],
                "ports": {
                  "in": [
                    {
                      "name": "clk"
                    },
                    {
                      "name": "pulse"
                    },
                    {
                      "name": "length"
                    }
                  ],
                  "out": [
                    {
                      "name": "data",
                      "range": "[63:0]",
                      "size": 64
                    }
                  ]
                }
              },
              "position": {
